@model Dictionary<string, string>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Web App Settings</h1>
</div>

<div class="row">
    @foreach (var kvp in @Model)
    {
        <div class="col-md-4">
            <h2>@kvp.Key</h2>
            <p>@kvp.Value</p>
        </div>
    }
</div>